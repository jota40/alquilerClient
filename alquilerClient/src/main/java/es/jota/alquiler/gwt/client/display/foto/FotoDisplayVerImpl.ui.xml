<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:HTMLPanel>
		<h1 class="page-header">Ver foto</h1>
		<div class="row">
			<div class="col-md-12">
				<div class="form-horizontal" role="form">
					<div class="row">
						<div class="col-md-6">
							<div class="form-group">
								<label class="col-sm-4 control-label">Foto</label>
								<div class="col-sm-8">
									<img ui:field="foto" class="img-responsive img-thumbnail"></img>
								</div>
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<label class="col-sm-4 control-label">Nombre</label>
								<div class="col-sm-8">
									<p ui:field="nombre" class="form-control-static"/>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-4 control-label">Tipo</label>
								<div class="col-sm-8">
									<p ui:field="type" class="form-control-static"/>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-4 control-label">Tama√±o</label>
								<div class="col-sm-8">
									<p ui:field="size" class="form-control-static"/>
								</div>
							</div>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-2 control-label">Titulo</label>
						<div class="col-sm-10">
							<p ui:field="titulo" class="form-control-static"/>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-2 control-label">Tag</label>
						<div class="col-sm-10">
							<p ui:field="tag" class="form-control-static"/>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-2 control-label">Vivenda</label>
						<div class="col-sm-10">
							<p ui:field="vivienda" class="form-control-static"/>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-2 control-label">Orden</label>
						<div class="col-sm-10">
							<p ui:field="orden" class="form-control-static"/>
						</div>
					</div>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>